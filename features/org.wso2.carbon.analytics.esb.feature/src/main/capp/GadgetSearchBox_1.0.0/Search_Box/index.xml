<?xml version="1.0" encoding="UTF-8" ?>
<Module>
    <ModulePrefs title="Auto Complete Search" description="g1" height="250">
        <Require feature="pubsub-2"/>
        <Require feature="dynamic-height"/>
    </ModulePrefs>
    <Content type="html">
        <![CDATA[
        <head>
          <script src="../commons/js/jquery-1.10.2.js"></script> 

          <link rel="stylesheet" type="text/css" href="/portal/libs/bootstrap_3.3.5/css/bootstrap.min.css" />
          <link rel="stylesheet" type="text/css" href="/portal/libs/theme-wso2_1.0/css/theme-wso2.css" />
           
          <link rel="stylesheet" type="text/css" href="../commons/css/font-awesome.min.css" />
          <link rel="stylesheet" type="text/css" href="../commons/css/autocomplete.css" />

          <script src="../commons/js/moment.min.js"></script>
          <script src="../commons/js/gadget-utils.js"></script>

          <!-- Typeahead JS -->
          <script src="../commons/js/typeahead.bundle.js"></script>

          
          <!-- Custom JS -->
          <script src="js/gadgetconf.js"></script>
          <script src="js/main.js"></script>

        </head>
        <body>
            
            <div class="form-group has-form-control-btn">
                <button class="form-control-btn" type="button" title="Search">
                    <i class="fa fa-search" aria-hidden="true"></i>
                </button>
                <input type="text" class="form-control typeahead" placeholder="Search...">
            </div> 

        </body>
        ]]>
    </Content>
</Module>